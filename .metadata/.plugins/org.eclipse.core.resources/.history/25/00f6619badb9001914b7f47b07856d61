<script type="text/javascript">

	
	
	function cancleBtnOnClick(){
		//스탬프 적립안하고 결제완료
		alert("결제완료. 잠시만 기다려주세요.");
	    window.opener.location.href="main";


		window.close();
	}
	
	function addStempBtnOnClick(){
		//스탬프 적립하고 결제완료
		alert("적립완료. 잠시만 기다려 주세요.");
		
		window.opener.location.href="main";
		
		window.close();
	}
</script>


</head>
